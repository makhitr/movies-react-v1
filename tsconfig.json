{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "ES2015",
    "module": "commonjs",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,  
    "forceConsistentCasingInFileNames": true,
  }
}
